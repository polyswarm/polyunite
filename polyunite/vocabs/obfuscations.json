{
    "group": "obfuscations",
    "children": {
        "packing": {
            "description": "Packing of the malware instance code, not including encryption.",
            "match": [{
                    "pattern": "(?<=[.])Pack(?=[.])"
                },
                "aspack",
                "asprotect",
                "badPE",
                "encoder",
                "encpk",
                "fsg",
                "genpack",
                "imptbl",
                "kriz",
                "krypt",
                "kryptik",
                "kryptk",
                "magistr",
                "malcrypt",
                "malob",
                "malpack",
                "matchicon",
                "md5sect",
                "nspack",
                "obfus",
                "obfusc",
                {
                    "pattern": "obfuscat(?:e|ed|or|ed|ing)"
                },
                "packed",
                "packer",
                "packing",
                "parite",
                "pecompact",
                "pespin",
                "petite",
                "polipos",
                "suspiciouspacker",
                "swizzor",
                "themida",
                "upack",
                "upx",
                "vbcrypt",
                "vbkrypt",
                "vbpack",
                "vmprotect",
                "wwpack",
                "xpack",
                "zpack"
            ]
        },
        "certificate": {
            "match": [
                "Revoked.Certificate",
                "Untrusted.Certificate"
            ]
        },
        "hidden": {
            "description": "Hidden code",
            "match": [
                "binder",
                "patched"
            ]
        },
        "code_encryption": {
            "description": "Encryption of the malware instance code.",
            "match": [
                "PE-Encrypt",
                "code-encryption",
                {
                    "pattern": "crypt(?:ed|er|ic|or)?"
                }
            ]
        },
        "unpacked": {
            "description": "Upacked binary or code from the original malware",
            "match": [
                "unpacked"
            ]
        }
    }
}

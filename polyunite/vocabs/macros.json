{
  "group": "macros",
  "children": {
    "ACAD": {
      "description": "Uses the AutoCAD application",
      "match": [
        "ACAD"
      ]
    },
    "ASX": {
      "description": "XML metafile of Windows Media .asf files",
      "match": [
        "ASX"
      ]
    },
    "HC": {
      "description": "HyperCard Apple scripts",
      "match": [
        "HC"
      ]
    },
    "HE": {
      "description": "macro scripting",
      "match": [
        "HE"
      ]
    },
    "MIME": {
      "description": "MIME packets",
      "match": [
        "MIME"
      ]
    },
    "CRX": {
      "description": "Chrome extension",
      "match": [
        "CRX"
      ]
    },
    "MSOffice": {
      "description": "Microsoft Office files",
      "match": [
        {
          "pattern": "(?i:MS(Excel|Word|Powerpoint|Outlook)((20)?\\d\\d)?)"
        },
        {
          "pattern": "(?i:Office((20)?\\d\\d)?)"
        },
        "Doc",
        "MSOffice",
        "SAgent",
        "macro",
        "macro.excel",
        "macro.word"
      ],
      "children": {
        "A97M": {
          "description": "Access 97, 2000, XP, 2003, 2007, and 2010 macros",
          "match": [
            "A97M"
          ]
        },
        "AM": {
          "description": "Runs as a VBA macro in the Microsoft Access application (version 97 or later)",
          "match": [
            "AM"
          ]
        },
        "CM": {
          "description": "Runs as a VBA macro in the Corel Draw! application (version 9.0 or later)",
          "match": [
            "CM"
          ]
        },
        "O97M": {
          "description": "Office 97, 2000, XP, 2003, 2007, and 2010 macros - those that affect Word, Excel, and Powerpoint",
          "match": [
              "O97",
              "O97M"
          ]
        },
        "OM": {
          "description": "Uses or runs in two or more applications in the Office 97 suite or later, including related products (Visio, Projects)",
          "match": [
            "OM"
          ]
        },
    "OLE": {
        "description": "Object linking and embedding",
        "match": [
            "OleNative",
            "OLE",
            "OLE2"
        ]
    },
    "PP97M": {
        "description": "PowerPoint 97, 2000, XP, 2003, 2007, and 2010 macros",
        "match": [
            "PP97M"
        ]
    },
    "PPM": {
        "description": "Runs as a VBS macro in Microsoft PowerPoint (version 97 or later)",
        "match": [
            "PPM"
        ]
    },
    "PUM": {
        "description": "Runs as a VBS macro in Microsoft Publisher (version 97 or later)",
        "match": [
            "PUM"
        ]
    },
    "V5M": {
        "description": "Visio5 macros",
        "match": [
            "V5M"
        ]
    },
    "W1M": {
        "description": "Word1Macro",
        "match": [
            "W1M"
        ]
    },
    "W2M": {
        "description": "Word2Macro",
        "match": [
            "W2M"
        ]
    },
    "W42": {
        "description": "Word2Macro",
        "match": [
            "W42"
        ]
    },
    "W97M": {
        "description": "Word 97, 2000, XP, 2003, 2007, and 2010 macros",
        "match": [
            "W97M"
        ]
    },
    "WM": {
        "description": "Word 95 macros",
        "match": [
            "WM"

    },
    "X97M": {
        "description": "Excel 97, 2000, XP, 2003, 2007, and 2010 macros",
        "match": [
            "X97M"
        ]
    },
    "XF": {
        "description": "Excel formulas",
        "match": [
            "XF"
        ]
    },
    "XM": {
        "description": "Excel 95 macros",
        "match": [
            "XM"
        ]
    },
    "Xls": {
        "match": [
            "Xls"
        ]
    }
}
},

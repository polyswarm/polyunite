    "MSOffice": {
      "description": "Microsoft Office files",
      "tags": {
        "platform": [
          "microsoft-office"
        ]
      },
      "match-exact": [
        "Doc",
        "Macro",
        "MSGate",
        "SAgent"
      ],
      "match-regex": [
        "(?:MS[.-]?)?(?:Office|Excel|Word|Powerpoint|Outlook)(?:XP|1997|97|1995|95|2000|2003|2007|2010|2013|2016|2020)?"
      ],
      "children": {
        "A97M": {
          "description": "Access 97, 2000, XP, 2003, 2007, and 2010 macros",
          "match-exact": [
            "A97M"
          ]
        },
        "AM": {
          "description": "Runs as a VBA macro in the Microsoft Access application (version 97 or later)",
          "match-exact": [
            "AM"
          ]
        },
        "CM": {
          "description": "Runs as a VBA macro in the Corel Draw! application (version 9.0 or later)",
          "match-exact": [
            "CM"
          ]
        },
        "O97M": {
          "description": "Office 97, 2000, XP, 2003, 2007, and 2010 macros - those that affect Word, Excel, and Powerpoint",
          "match-exact": [
            "O2000M",
            "O2007M",
            "O2020M",
            "O97",
            "O97M",
            "Office2k"
          ]
        },
        "OLE": {
          "description": "Object linking and embedding",
          "match-exact": [
            "OLE",
            "OLE2",
            "OleNative"
          ]
        },
        "OM": {
          "description": "Uses or runs in two or more applications in the Office 97 suite or later, including related products (Visio, Projects)",
          "match-exact": [
            "OM"

    "ACAD": {
      "description": "Uses the AutoCAD application",
      "match-exact": [
        "ACAD",
        "AutoCAD",
        "AutoLisp"
      ]
    },
    "ASX": {
      "description": "XML metafile of Windows Media .asf files",
      "match-exact": [
        "ASX"
      ]
    },
    "CRX": {
      "description": "Chrome extension",
      "match-exact": [
        "CRX"
      ]
    },
    "FONT": {
      "description": "Font files",

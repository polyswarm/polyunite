{
  "group_name": "HEURISTICS",
  "children": {
    "damaged": {
      "match-exact": [
        "damaged copy of",
        "damaged-copy-of",
        "damaged.copy.of"
      ],
      "match-regex": [
        "Damaged[._]?File"
      ]
    },
    "generic": {
      "description": "Generic virus detection",
      "match-exact": [
        "advml",
        "attribute",
        "behaveslike",
        "bloodhound",
        "cloud",
        "Gen",
        "generic",
        "genetic",
        "genmalicious",
        "Heuri",
        "MaliciousHeuristic",
        "MGen",
        "NewHeur",
        "score",
        "static",
        "undefined",
        "VBGeneric"
      ],
      "match-regex": [
        "[gG]eneric[0-9]+",
        "[hH]eur[0-9]*",
        "heuristics?",
        "Siggen[0-9]?"
      ]
    },
    "heurfamily": {
      "description": "Contains characteristics usual of a particular class of viruses",
      "match-exact": [
        "agent",
        "agent_",
        "agentad",
        "agentb",
        "Delf",
        "family",
        "foreign",
        "genome",
        "kcloud",
        "maltrec",
        "memscan",
        "sape",
        "symvt",
        "undefined",
        "vgeneric"
      ],
      "match-regex": [
        "Agent[0-9]+"
      ]
    }
  }
}

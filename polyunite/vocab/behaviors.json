{
  "group_name": "BEHAVIORS",
  "children": {
    "anti_firewall": {
      "tags": {
        "behavior": [
          "disable-firewall"
        ]
      },
      "match-exact": [
        "AntiFW"
      ]
    },
    "anti_virtual_machine": {
      "tags": {
        "malware-capabilities": [
          "anti-vm"
        ]
      },
      "match-exact": [
        "AntiVM"
      ]
    },
    "antiarj": {
      "match-exact": [
        "AntiArj"
      ]
    },
    "autorun": {
      "tags": {
        "behavior": [
          "persist-after-system-reboot"
        ]
      },
      "match-exact": [
        "Autorun"
      ]
    },
    "capture_system_memory": {
      "tags": {
        "behavior": [
          "capture-system-memory"
        ]
      },
      "match-exact": [
        "MemScan"
      ],
      "match-combine": [
        {
          "parts": [
            [
              "MemScan"
            ],
            [
              "Trojan",
              "Backdoor"
            ]
          ]
        }
      ]
    },
    "certificate": {
      "match-combine": [
        {
          "parts": [
            [
              "Revoked",
              "Untrusted"
            ],
            [
              "Certificate"
            ]
          ],
          "separator": "word-break"
        }
      ]
    },
    "code_encryption": {
      "description": "Encryption of the malware instance code.",
      "match-exact": [
        "code-encryption",
        "encoder",
        "encpk",
        "kryptk",
        "malcrypt",
        "PE-Encrypt"
      ],
      "match-combine": [
        {
          "parts": [
            [
              "Crypt",
              "Krypt"
            ],
            [
              "ed",
              "ic",
              "or",
              "er"
            ]
          ]
        }
      ]
    },
    "erase_data": {
      "tags": {
        "behavior": [
          "erase-data"
        ]
      },
      "match-exact": [
        "KillFiles"
      ]
    },
    "evades_av": {
      "tags": {
        "malware-capabilities": [
          "evades-av"
        ]
      },
      "match-exact": [
        "AVKill",
        "Countermeasure",
        "KillAV"
      ],
      "match-combine": [
        {
          "parts": [
            [
              "AV",
              "AntiVirus"
            ],
            [
              "Killer"
            ]
          ],
          "separator": "word-break"
        }
      ]
    },
    "exfiltrates_data": {
      "description": "able to gather, prepare, (possibly obfuscate) data and transmit it to exfiltration points.",
      "tags": {
        "malware-capabilities": [
          "exfiltrates-data"
        ]
      },
      "match-exact": [
        "Notifier"
      ],
      "children": {
        "exfiltrates_data_trojan": {
          "tags": {
            "malware-type": [
              "trojan"
            ]
          },
          "match-combine": [
            {
              "parts": [
                [
                  "Trojan"
                ],
                [
                  "Notifier"
                ]
              ],
              "separator": "word-break"
            }
          ]
        }
      }
    },
    "generic": {
      "match-exact": [
        "Gen",
        "Generic",
        "Heuri",
        "Heuristic",
        "Heuristics"
      ]
    },
    "hidden": {
      "description": "Hidden code",
      "match-exact": [
        "binder",
        "patched"
      ]
    },
    "intercept_manipulate_network_traffic": {
      "description": "Intercepts and/or manipulates network traffic going to or originating from the system on which the malware instance is executing.",
      "tags": {
        "behavior": [
          "intercept-manipulate-network-traffic"
        ]
      },
      "match-exact": [
        "Sniffer"
      ],
      "children": {
        "intercept_manipulate_network_traffic_trojan": {
          "tags": {
            "malware-type": [
              "trojan"
            ]
          },
          "match-combine": [
            {
              "parts": [
                [
                  "Trojan"
                ],
                [
                  "Sniffer"
                ]
              ],
              "separator": "word-break"
            }
          ]
        }
      }
    },
    "kill_process": {
      "tags": {
        "malware-action": [
          "kill-process"
        ]
      },
      "match-exact": [
        "ProcessKiller",
        "KillProc"
      ]
    },
    "kill_windows": {
      "tags": {
        "malware-action": [
          "kill-windows"
        ]
      },
      "match-exact": [
        "KillWin"
      ]
    },
    "network_traffic_redirector": {
      "tags": {
        "behavior": [
          "intercept-manipulate-network-traffic"
        ]
      },
      "match-exact": [
        "Redirector"
      ]
    },
    "packing": {
      "description": "Packing of the malware instance code, not including encryption.",
      "match-exact": [
        "Obfus",
        "Obfusc",
        "Obfuscate",
        "Obfuscator",
        "Pack",
        "SuspiciousPacker",
        "Swizzor",
        "Themida",
        "UPX",
        "aspack",
        "asprotect",
        "badPE",
        "fsg",
        "genpack",
        "imptbl",
        "kriz",
        "magistr",
        "malob",
        "malpack",
        "matchicon",
        "md5sect",
        "nspack",
        "parite",
        "pecompact",
        "pespin",
        "petite",
        "polipos",
        "upack",
        "vbpack",
        "vmprotect",
        "wwpack",
        "xpack",
        "zpack"
      ],
      "match-combine": [
        {
          "parts": [
            [
              "Obfuscat",
              "Pack"
            ],
            [
              "ing",
              "ed",
              "er"
            ]
          ]
        }
      ],
      "children": {
        "packing_trojan": {
          "tags": {
            "malware-type": [
              "trojan"
            ]
          },
          "match-combine": [
            {
              "parts": [
                [
                  "Trojan"
                ],
                [
                  "Crypter"
                ]
              ],
              "separator": "word-break"
            }
          ]
        }
      }
    },
    "suspicious": {
      "description": "Suspicious behavior without signature match",
      "match-exact": [
        "Abuse-Worry",
        "DangerousObject",
        "Mailfinder",
        "Obscure",
        "Reputation",
        "Suspect",
        "Suspected",
        "Suspected",
        "Suspicious",
        "Threat",
        "Variant",
        "behaves like ",
        "confidence",
        "highconfidence",
        "looks like ",
        "modification ",
        "modification of ",
        "possible ",
        "possible-Threat",
        "possibly ",
        "probable ",
        "probably ",
        "variant of "
      ]
    },
    "unpacked": {
      "description": "Upacked binary or code from the original malware",
      "match-exact": [
        "Raw",
        "unpacked"
      ]
    }
  }
}

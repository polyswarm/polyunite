{
  "group": "OPERATING_SYSTEMS",
  "children": {
    "Android": {
      "description": "Android Phone OS",
      "match": [
        "Andr",
        "Android",
        "AndroidOS"
      ]
    },
    "FTP": {
      "description": "Uses File Transfer Protocol",
      "match": [
        "FTP"
      ]
    },
    "Boot": {
      "description": "Uses or resides in the Master Boot Record (MBR) or DOS Boot Sector of an operating system",
      "match": [
        "Boot",
        "TSR",
        "Boot-DOS"
      ]
    },
    "DOS": {
      "description": "MS-DOS",
      "match": [
        {
          "pattern": "(?-i:DOS|Dos|dos)(?:[.-](?:vcl|VCL))?"
        },
        "MSDOS",
        "FDOS",
        "DOSCOM",
        "PS-MPC"
      ]
    },
    "EPOC": {
      "description": "Psion devices",
      "match": [
        "EPOC"
      ]
    },
    "FreeBSD": {
      "description": "FreeBSD platform",
      "match": [
        "FreeBSD"
      ]
    },
    "Multi": {
      "match": [
        "Any",
        "Application",
        "Legacy",
        "Multi",
        "System",
        "multios"
      ]
    },
    "Linux": {
      "description": "Linux platform",
      "match": [
        "Linux"
      ],
      "children": {
        "linux_arm": {
          "tags": {
            "processor-architecture": [
              "arm"
            ],
            "match": [
              {
                "pattern": "(?i:ELF.?(Linux|ARM|x)?\\d*)",
                "processor-architecture": "arm"
              }
            ]
          }
        }
      }
    },
    "MacOSX": {
      "description": "MACOSX 10.0 or later",
      "match": [
        {
          "pattern": "Mac(?:\\_|\\s|\\.)?OS(?:\\_|\\s|\\.)?X?"
        },
        "Mac",
        "MacOS",
        "MacOSX",
        "OSX"
      ]
    },
    "OS2": {
      "description": "OS2 platform",
      "match": [
        "OS2"
      ]
    },
    "Palm": {
      "description": "Palm operating system",
      "match": [
        "Palm"
      ]
    },
    "Symbian": {
      "description": "Symbian",
      "match": [
        "Symbian"
      ]
    },
    "Script": {
      "description": "Interpreted",
      "match": [
        "Script"
      ]
    },
    "Solaris": {
      "description": "System V-based Unix platforms",
      "match": [
        "Solaris"
      ]
    },
    "SunOS": {
      "description": "Unix platforms 4.1.3 or lower",
      "match": [
        "SunOS"
      ]
    },
    "SymbOS": {
      "description": "Symbian operating system",
      "match": [
        "SymbOS"
      ]
    },
    "UKP": {
      "match": [
        "UKP"
      ]
    },
    "UNIX": {
      "description": "general Unix platforms",
      "match": [
        "UNIX"
      ]
    },
    "Windows": {
      "description": "Microsoft windows",
      "match": [
        "Win",
        "Win32.64",
        "Win32/64",
        {
          "pattern": "\\bWScr\\b"
        },
        "Windows"
      ],
      "children": {
        "internet_explorer": {
          "match": [
            "ActiveX"
          ]
        },
        "IIS": {
          "description": "Microsoft Windows IIS Webserver",
          "match": [
            "IIS"
          ]
        },
        "Win16": {
          "description": "Win16 (3.1) platform",
          "match": [
            "Win16"
          ]
        },
        "Win2K": {
          "description": "Windows 2000 platform",
          "match": [
            "Win2K"
          ]
        },
        "Win32": {
          "description": "Windows 32-bit platform",
          "processor-architecture": "x86",
          "match": [
            "W32",
            "Win32"
          ]
        },
        "Win64": {
          "description": "Windows 64-bit platform",
          "processor-architecture": "x86-64",
          "match": [
            "W64",
            "Win64"
          ]
        },
        "Win95": {
          "description": "Windows 95, 98 and ME platforms",
          "match": [
            "W95",
            "Win95"
          ]
        },
        "Win98": {
          "description": "Windows 98 platform only",
          "match": [
            "W98",
            "W97",
            "Win9x",
            "Win98"
          ]
        },
        "WinCE": {
          "description": "Windows CE platform",
          "match": [
            "WinCE"
          ]
        },
        "WinNT": {
          "description": "WinNT",
          "match": [
            "WinNT"
          ]
        }
      }
    },
    "iPhoneOS": {
      "description": "IPhone",
      "match": [
        "IOS",
        "iPhoneOS"
      ]
    }
  }
}
